<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="foundit.foundit.FragRegistro_Comercio">


    <!-- TODO: Update blank fragment layout -->
    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal"
        android:clipToPadding="true"
        android:fillViewport="true"
        android:scrollbarStyle="insideOverlay"
        app:layout_scrollFlags="scroll"
        tools:ignore="ExtraText">
        app:layout_behavior="@string/appbar_scrolling_view_behavior"

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_behavior="@string/appbar_scrolling_view_behavior"
            tools:context="foundit.foundit.Registro_Comercio"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="81dp"
            tools:showIn="@layout/activity_registro__comercio">

            <EditText
                android:id="@+id/nombreDelComercioText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/plainTextNombre"
                android:layout_marginStart="17dp"
                android:layout_marginTop="13dp"
                android:ems="10"
                android:hint="Nombre"
                android:inputType="textPersonName"
                android:textAppearance="@style/TextAppearance.AppCompat" />

            <TextView
                android:id="@+id/plainTextNombre"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignStart="@+id/nombreDelComercioText"
                android:layout_marginTop="9dp"
                android:text="* Nombre del Comercio:"
                android:textStyle="bold"
                android:visibility="visible" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/nombreDelComercioText"
                android:layout_below="@+id/nombreDelComercioText"
                android:layout_marginTop="15dp"
                android:text="* Pais:"
                android:textStyle="bold" />

            <Spinner
                android:id="@+id/PaisSpinner"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/nombreDelComercioText"
                android:layout_alignStart="@+id/textView3"
                android:layout_below="@+id/textView3"
                android:layout_marginTop="8dp"
                android:entries="@array/paises"/>

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/PaisSpinner"
                android:layout_below="@+id/PaisSpinner"
                android:layout_marginTop="12dp"
                android:text="* Direccion Postal:"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/direccionText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView5"
                android:layout_below="@+id/textView5"
                android:layout_marginTop="15dp"
                android:ems="10"
                android:hint="Calle las monas, 33"
                android:inputType="textPersonName"
                android:textAppearance="@style/TextAppearance.AppCompat" />

            <TextView
                android:id="@+id/ciudadPlainText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/direccionText"
                android:layout_below="@+id/direccionText"
                android:layout_marginTop="8dp"
                android:text="* Ciudad"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/ciudadText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/textView5"
                android:layout_alignStart="@+id/ciudadPlainText"
                android:layout_below="@+id/ciudadPlainText"
                android:layout_marginTop="12dp"
                android:ems="10"
                android:hint="Valencia"
                android:inputType="textPersonName"
                android:textAppearance="@style/TextAppearance.AppCompat" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/codPostalText"
                android:layout_alignStart="@+id/codPostalText"
                android:layout_alignTop="@+id/ciudadPlainText"
                android:text="* Codigo Postal:"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/codPostalText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/ciudadText"
                android:layout_alignBottom="@+id/ciudadText"
                android:layout_marginStart="9dp"
                android:layout_toEndOf="@+id/plainTextNombre"
                android:ems="10"
                android:hint="46111"
                android:inputType="textPersonName"
                android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/ciudadText"
                android:layout_below="@+id/ciudadText"
                android:layout_marginTop="12dp"
                android:text="* Teléfono:"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/telfText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView10"
                android:layout_below="@+id/textView10"
                android:layout_marginTop="11dp"
                android:ems="10"
                android:hint="619444555"
                android:inputType="textPersonName"
                android:textAppearance="@style/TextAppearance.AppCompat" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/telfText"
                android:layout_below="@+id/telfText"
                android:layout_marginTop="13dp"
                android:text="* Categoria:"
                android:textStyle="bold" />

            <Spinner
                android:id="@+id/categoriaSpinner"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/telfText"
                android:layout_alignStart="@+id/textView11"
                android:layout_below="@+id/textView11"
                android:layout_marginTop="11dp"
                android:entries="@array/Categorias"/>

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/categoriaSpinner"
                android:layout_below="@+id/categoriaSpinner"
                android:layout_marginTop="13dp"
                android:hint="Direccion de correo electrónico"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/emailText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView13"
                android:layout_below="@+id/textView13"
                android:layout_marginTop="9dp"
                android:ems="10"
                android:hint="correo@correo.com"
                android:inputType="textPersonName"
                android:textAppearance="@style/TextAppearance.AppCompat" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/emailText"
                android:layout_below="@+id/emailText"
                android:layout_marginTop="12dp"
                android:text="Sitio web"
                android:textAllCaps="false"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/webText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView14"
                android:layout_below="@+id/textView14"
                android:layout_marginTop="9dp"
                android:ems="10"
                android:hint="www.casamanolo.com"
                android:inputType="textPersonName"
                android:textAppearance="@style/TextAppearance.AppCompat" />

            <Button
                android:id="@+id/registro"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/webText"
                android:layout_below="@+id/webText"
                android:layout_marginTop="9dp"
                android:text="Registro" />

            <Button
                android:id="@+id/cancelar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignTop="@+id/registro"
                android:layout_marginEnd="20dp"
                android:text="Cancelar" />

        </RelativeLayout>
    </ScrollView>

</FrameLayout>
